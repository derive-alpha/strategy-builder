{
    "include": [
        "src/javascript/**/*"
    ],
    "exclude": [
        "build/**/*",
        "node_modules/**/*"
    ],
    "compilerOptions": {
        "target": "es6",
        "jsx": "react",
        "sourceMap": true,
        "moduleResolution": "node",
        "incremental": true,
"esModuleInterop": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "strict": true,
        "importsNotUsedAsValues": "preserve"
    }
}
